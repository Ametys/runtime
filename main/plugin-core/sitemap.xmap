        	<map:generator name="dispatcher" src="org.ametys.runtime.plugins.core.dispatcher.DispatchGenerator"/>
        	<map:generator name="i18n" src="org.ametys.runtime.util.I18nGenerator" logger="org.ametys.runtime.util.I18nGenerator"/>
        </map:generators>
        
        <map:serializers>
            <map:serializer name="zip" src="org.ametys.runtime.cocoon.ZipArchiveNGSerializer" logger="org.ametys.runtime.plugins.core.administrator.sitemap.serializer.zip"/>
        </map:serializers>
        
		<map:actions>
            <map:action name="set-header" src="org.apache.cocoon.acting.HttpHeaderAction" logger="org.ametys.runtime.plugins.core.administrator.sitemap.action.httpHeader"/>

