<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:element name="workspace" type="workspace"/>

    <xsd:complexType name="workspace">
        <xsd:sequence>
            <xsd:element name="dependencies" type="dependencies" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="dependencies">
        <xsd:sequence>
            <xsd:element name="dependency" type="xsd:string" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>